
local player = game:GetService("Players").LocalPlayer


local numericalAttributes = {
    Att_Conservation = 7,
    Att_Dexterity = 7,
    Att_Efficiency = 15,
    Att_Fortitude = 5,
    Att_Strength = 5
}

for attributeName, value in pairs(numericalAttributes) do
    player:SetAttribute(attributeName, value)
end




